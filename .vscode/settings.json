{    
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "java": "cd $dir && java $fileName",
  },
  
  "java.project.referencedLibraries": [
    "/var/libs/stdlib.jar"
  ],

  "files.exclude": {
    "**/.github": true,
    "**/.devcontainer": true,
    "**/.vscode": true
  }
}
